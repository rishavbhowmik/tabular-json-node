{
  "name": "tabular-json-node",
  "version": "0.0.0",
  "description": "Convert JSON data into tabular format and thus binary into a format. Alternative to BSON",
  "main": "./lib/index.js",
  "type": "commonjs",
  "types": "./lib/index.d.ts",
  "directories": {
    "lib": "lib",
    "example": "examples",
    "test": "tests",
    "doc": "docs"
  },
  "scripts": {
    "startDev": "tsc -w",
    "compile": "tsc",
    "test": "node ./node_modules/mocha/bin/mocha ./tests/index.ts --require ts-node/register --recursive",
    "privatePublish": "npm publish --registry https://npm.pkg.jetbrains.space/indianfoodrecipes/p/jovial-open-source/tabular-json-node/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:jovial-chat/tabular-json-js.git"
  },
  "keywords": [
    "json",
    "jsonToBinary",
    "jsonToTable",
    "jsonToBuffer"
  ],
  "author": "Rishav Bhowmik",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/jovial-chat/tabular-json-js/issues"
  },
  "homepage": "https://github.com/jovial-chat/tabular-json-js#readme",
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "mocha": "^8.3.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
